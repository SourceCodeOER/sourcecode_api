{
  "name": "exercises_library",
  "version": "1.0.0",
  "description": "API for Exercises Library",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env DEBUG=express-sequelize node ./bin/www",
    "seeders": "sequelize-cli db:seed:all --debug",
    "migrate": "sequelize-cli db:migrate",
    "createDatabase": "sequelize db:create",
    "revertMigration": "npx sequelize-cli db:migrate:undo",
    "reverSeed": "npx sequelize-cli db:seed:undo",
    "createMigration": "sequelize migration:create --name default_migration",
    "generateClient": "npx openapi-generator generate -g typescript-axios -i api.yml -o out"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "bluebird": "^3.7.1",
    "body-parser": "latest",
    "cookie-parser": "^1.4.4",
    "cross-env": "^6.0.3",
    "debug": "latest",
    "express": "^4.17.1",
    "express-openapi-validator": "^2.14.5",
    "jsonwebtoken": "^8.5.1",
    "lodash.partition": "^4.6.0",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-json": "^1.2.0",
    "passport-jwt": "^4.0.0",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.2",
    "sequelize-cli": "^5.5.1"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^1.0.2-4.2.0",
    "@types/express": "^4.17.2",
    "@types/sequelize": "^4.28.6",
    "redoc-cli": "^0.9.2"
  }
}
